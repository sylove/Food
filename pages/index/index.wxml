<!--index.wxml-->
<view class="container">
  <view>
    <swiper class="swiper_box" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{banners}}" wx:key="id">
        <swiper-item >
          <image src="{{item.picUrl}}" />
        </swiper-item>
      </block>
    </swiper>
  </view>
</view>
